{
    "name": "lunarphp/core",
    "description": "Lunar Laravel e-commerce core functionality.",
    "keywords": ["lunar", "laravel", "ecommerce", "e-commerce", "headless", "store", "shop", "cart"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
          "name": "Lunar",
          "homepage": "https://lunarphp.io/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lunar\\": "src",
            "Lunar\\Database\\Factories\\": "database/factories",
            "Lunar\\Database\\Seeders\\": "database/seeders",
            "Lunar\\Database\\State\\": "database/state"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lunar\\LunarServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-exif": "*",
        "ext-intl": "*",
        "laravel/framework": "^10.0|^11.0",
        "spatie/laravel-medialibrary": "^11.0.0",
        "spatie/laravel-activitylog": "^4.4",
        "laravel/scout": "^10.0",
        "cartalyst/converter": "^8.0|^9.0",
        "kalnoy/nestedset": "^v6.x-dev",
        "doctrine/dbal": "^3.6",
        "lukascivil/treewalker": "0.9.1",
        "spatie/php-structure-discoverer": "^2.0",
        "spatie/laravel-blink": "^1.7"
    }
}
